<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="e_pos_mrp.ControlButtons" t-inherit="point_of_sale.ControlButtons" t-inherit-mode="extension">
        <xpath expr="//t[@t-if='props.showRemainingButtons']//div" position="inside">
            <button class="btn btn-secondary btn-lg py-5" t-on-click="() => this.createManofacture()">
                <i class="fa fa-wrench me-1" role="img" /> Create Manufacture
            </button>
        </xpath>
    </t>
</templates>
