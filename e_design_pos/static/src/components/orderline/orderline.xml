<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="e_design_pos.Orderline" t-inherit="point_of_sale.Orderline" t-inherit-mode="extension">
       <xpath expr="//div[hasclass('product-name')]" position="inside">
            <t t-if="this.props.line?.design_display_name">
                (<t t-esc="this.props.line.design_display_name"/>)
            </t>
       </xpath>
    </t>
</templates>
